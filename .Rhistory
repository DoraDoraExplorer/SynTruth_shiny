return(gep$gep)
}
get_hrs <- function(gep){
gep$group <- as.factor(gep$group)
gep$group <- relevel(gep$group, ref = "NB0")
coxph <- coxph(Surv(surv_time, status) ~ group,
data = gep)
hrs <- round(exp(coxph$coefficients), 2)
return(hrs)
}
